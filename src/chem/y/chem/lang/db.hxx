"water   : <=> [H+]+[HO-] : '1e-14'",
"acetic  : [EtCOOH]  <=> [H+] + [EtCOO-]  : '10^(-4.76)'",
"benzoic : [PhiCOOH] <=> [H+] + [PhiCOO-] : '10^(-4.20)'",
"formic  : [MeCOOH]  <=> [H+] + [MeCOO-]  : '10^(-3.75)'",
"ammoniac: [NH4+]    <=> [H+] + [NH3]     : '10^(-9.18)'",
"sulfuric: [H2SO4]   <=> 2[H+] + [SO4--]  : '10^(1.1)'",

"oxalic1 : [OxH2]    <=> [H+] + [OxH-]    : '10^(-1.20)'",
"oxalic2 : [OxH-]    <=> [H+] + [Ox--]    : '10^(-4.30)'",

"citric1 : [CitH3]   <=> [H+] + [CitH2-]    : '10^(-3.13)'",
"citric2 : [CitH2-]  <=> [H+] + [CitH--]    : '10^(-4.76)'",
"citric3 : [CitH--]  <=> [H+] + [Cit---]    : '10^(-6.40)'",

"phosphoric1: [H3PO4]  <=> [H+] + [H2PO4-]    : '10^(-2.148)'",
"phosphoric2: [H2PO4-] <=> [H+] + [HPO4--]    : '10^(-7.198)'",
"phosphoric3: [HPO4--] <=> [H+] + [PO4---]    : '10^(-12.319)'",

"edta1 : [YH4]   <=> [H+] + [YH3-]  : '10^(-2.0)'",
"edta2 : [YH3-]  <=> [H+] + [YH2--] : '10^(-2.7)'",
"edta3 : [YH2--] <=> [H+] + [YH---] : '10^(-6.2)'",
"edta4 : [YH---] <=> [H+] + [Y----] : '10^(-10.3)'",

"abcd : [a]+[b]   <=> [c]+[d]   : '1.28' ",
"abis : [a]+2[b]  <=> 3[c]+5[d] : '2.72' ",
"ater : 2[a]+3[b] <=> 5[c]+7[d] : '3.14' "

